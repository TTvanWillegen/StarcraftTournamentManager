<SplitPane dividerPositions="0.33, 0.66"
           focusTraversable="true" prefHeight="-1.0"
           prefWidth="-1.0">
    <items>
        <SplitPane dividerPositions="0.5"
                   orientation="VERTICAL">
            <items>
                <ToolBar prefHeight="40.0"
                         prefWidth="200.0">
                    <items>
                        <Pane prefWidth="20.0"/>
                        <Label text="Teams"/>
                        <Pane prefWidth="300.0"/>
                        <Button alignment="CENTER"
                                contentDisplay="CENTER"
                                mnemonicParsing="false"
                                text="+"/>
                    </items>
                </ToolBar>
                <ScrollPane>
                    <content>
                        <AnchorPane/>
                    </content>
                </ScrollPane>
            </items>
        </SplitPane>
        <SplitPane dividerPositions="0.5" layoutX="10.0"
                   layoutY="10.0"
                   orientation="VERTICAL">
            <items>
                <ToolBar prefHeight="40.0"
                         prefWidth="200.0">
                    <items>
                        <Pane prefWidth="20.0"/>
                        <Label text="Team Settings"/>
                    </items>
                </ToolBar>
                <ScrollPane>
                    <content>
                        <AnchorPane/>
                    </content>
                </ScrollPane>
            </items>
        </SplitPane>
        <SplitPane dividerPositions="0.5" layoutX="10.0"
                   layoutY="10.0"
                   orientation="VERTICAL">
            <items>
                <ToolBar prefHeight="40.0"
                         prefWidth="200.0">
                    <items>
                        <Pane prefWidth="20.0"/>
                        <Label text="Ranking"/>
                    </items>
                </ToolBar>
                <GridPane prefHeight="40.0">
                    <columnConstraints>
                        <ColumnConstraints
                                hgrow="SOMETIMES"
                                minWidth="10.0"
                                prefWidth="100.0"/>
                        <ColumnConstraints
                                hgrow="SOMETIMES"
                                minWidth="10.0"
                                prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0"
                                        prefHeight="30.0"
                                        vgrow="SOMETIMES"/>
                    </rowConstraints>
                </GridPane>
            </items>
        </SplitPane>
    </items>
